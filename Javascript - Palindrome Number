/* Javascript attempt at https://leetcode.com/problems/palindrome-number/*/
/**
 * @param {number} x
 * @return {boolean}
 */
var isPalindrome = function(x) {
    var y = x.toString().split('').reverse().join('');
    var z = parseInt(y);
    if (z == x){
        return true;
    }
    else {
        return false;
    }
};
