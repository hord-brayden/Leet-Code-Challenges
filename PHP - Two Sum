/*PHP attempt at https://leetcode.com/problems/two-sum/*/
/**
 * @param {number[]} nums
 * @param {number} target
 * @return {number[]}
 */
var twoSum = function(nums, target) {
    var i = 0;
    while (i <= nums.length){
        var j = i + 1;
        while (j <= nums.length) {
            if (nums[i] + nums[j] == target){ 
                var ans = [i,j];
                return ans;
            }
            j++;
        }
        i++;
    }
};
